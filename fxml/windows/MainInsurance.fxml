            </Label>
         </children>
      </AnchorPane>
      <HBox prefHeight="30.0" style="-fx-background-color: #d8dce0;" VBox.vgrow="ALWAYS" />
      <AnchorPane styleClass="grid-pane" stylesheets="@../pics/gridPaneStyle.css">
         <children>
            <AnchorPane layoutX="17.0" layoutY="65.0">
               <children>
                  <VBox alignment="CENTER" minWidth="300.0" spacing="10.0">
                     <children>
                        <Button fx:id="btnEmulateCar" mnemonicParsing="false" onAction="#btnEmulatePressed" prefHeight="30.0" prefWidth="200.0" styleClass="task-button" stylesheets="@../pics/style.css" text="Car Ins." textFill="WHITE">
