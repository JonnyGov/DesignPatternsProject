                        </Button>
                        <Button fx:id="btnEmulateLife" mnemonicParsing="false" onAction="#btnEmulatePressed" prefHeight="30.0" prefWidth="200.0" styleClass="task-button" stylesheets="@../pics/style.css" text="Life Ins." textFill="WHITE">
